@article{NUSSLIN202065,
title = {Wilhelm Conrad Röntgen: The scientist and his discovery},
journal = {Physica Medica},
volume = {79},
pages = {65-68},
year = {2020},
note = {125 Years of X-Rays},
issn = {1120-1797},
doi = {https://doi.org/10.1016/j.ejmp.2020.10.010},
url = {https://www.sciencedirect.com/science/article/pii/S1120179720302532},
author = {Fridtjof Nüsslin},
keywords = {Wilhelm Conrad Röntgen, Discovery of X-rays, Physics history of the 19th century},
abstract = {Röntgen's discovery of a new type of radiation is the epochal event in a series of highlights of physics emerging within only a few decades of the late 19th century. As these discoveries are directly or indirectly rooting in the study of the phenomenon of electric discharge in gases a brief look at the physics scenario in the immediate pre-X-ray era is presented in the first section. Rather than just the fortune with the bold it is Röntgen’s character as a diligent, self-critical and ingenious scientist which made his discovery possible. This will be illustrated in the sections on Röntgen’s personal life and some specific details of his experiments leading to the discovery of X-rays. Finally, a short overview is given on the potential and the large variety of applications of X-rays.}
}
